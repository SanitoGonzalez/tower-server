COMPOSE_PROJECT_NAME=tower

TOWER_DEBUG=true

TOWER_MAIN_LISTEN_PORT=30000
TOWER_MAIN_TICK_INTERVAL_MILLISECONDS=125

TOWER_AUTH_JWT_KEY_FILE=/run/secrets/auth-jwt-key
TOWER_AUTH_JWT_ALGORITHM=HS256
